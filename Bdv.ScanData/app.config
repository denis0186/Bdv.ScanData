<?xml version="1.0" encoding="utf-8"?>
<configuration>
<startup><supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0"/></startup>

  <!--Секция глобальных настроек программы-->
  <appSettings>
    <!--Скорость COM-порта 1200/2400/4800/9600/19200 и т.д.-->
    <add key="SerialPortBaudRate" value="9600"/>
    <!--Паритет COM-порта 0 - None, 1 - Odd, 2 - Even, 3 - Mark, 4 - Space-->
    <add key="SerialPortParity" value="0"/>
    <!--Число бит данных COM-порта -->
    <add key="SerialPortDataBits" value="8"/>
	  <!--Число стоп бит COM-порта -->
	  <add key="SerialPortStopBits" value="1"/>
  </appSettings>
  
</configuration>
